{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/main/devbox.schema.json",
  "packages": [
    "python@3.11",
    "dotnet-sdk_10@latest",
    "nodejs@20",
    "git",
    "uv@latest",
    "go@1.22.2"
  ],
  "shell": {
    "init_hook": [],
    "scripts": {
      "test": [
        "echo 'Running tests...'",
        "python --version && echo 'Python OK'",
        "dotnet --version && echo '.NET OK'",
        "go version && echo 'Go OK'",
        "node --version && echo 'Node.js OK'",
        "tsc --version && echo 'TypeScript OK'"
      ],
      "setup": [
        "echo 'Setting up development environment...'",
        "pip install --upgrade pip",
        "npm install -g npm@latest"
      ]
    }
  }
}
